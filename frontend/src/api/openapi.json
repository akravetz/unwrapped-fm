  "components": {
    "schemas": {
      "AnalysisStatus": {
        "type": "string",
        "enum": [
          "pending",
          "processing",
          "completed",
          "failed"
        ],
        "title": "AnalysisStatus",
        "description": "Status values for music analysis background tasks."
      },
      "AnalysisStatusResponse": {
        "properties": {
          "analysis_id": {
            "type": "integer",
            "title": "Analysis Id"
          },
          "status": {
            "$ref": "#/components/schemas/AnalysisStatus"
          },
          "error_message": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Error Message"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At"
          },
          "started_at": {
            "anyOf": [
              {
                "type": "string",
                "format": "date-time"
              },
              {
                "type": "null"
              }
            ],
            "title": "Started At"
          },
          "completed_at": {
            "anyOf": [
              {
                "type": "string",
                "format": "date-time"
              },
              {
                "type": "null"
              }
            ],
            "title": "Completed At"
          }
        },
        "type": "object",
        "required": [
          "analysis_id",
          "status",
          "created_at"
        ],
        "title": "AnalysisStatusResponse",
        "description": "Analysis status response for polling endpoint."
      },
      "BeginAnalysisResponse": {
        "properties": {
          "analysis_id": {
            "type": "integer",
            "title": "Analysis Id"
          },
          "status": {
            "$ref": "#/components/schemas/AnalysisStatus"
          }
        },
        "type": "object",
        "required": [
          "analysis_id",
          "status"
        ],
        "title": "BeginAnalysisResponse",
        "description": "Response for begin analysis endpoint."
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "MusicAnalysisResponse": {
        "properties": {
          "rating_text": {
            "type": "string",
            "title": "Rating Text"
          },
          "rating_description": {
            "type": "string",
            "title": "Rating Description"
          },
          "x_axis_pos": {
            "type": "number",
            "title": "X Axis Pos"
          },
          "y_axis_pos": {
            "type": "number",
            "title": "Y Axis Pos"
          },
          "share_token": {
            "type": "string",
            "title": "Share Token"
          },
          "analyzed_at": {
            "type": "string",
            "format": "date-time",
            "title": "Analyzed At"
          }
        },
        "type": "object",
        "required": [
          "rating_text",
          "rating_description",
          "x_axis_pos",
          "y_axis_pos",
          "share_token",
          "analyzed_at"
        ],
        "title": "MusicAnalysisResponse",
        "description": "Music analysis API response model."
      },
      "PublicAnalysisResponse": {
        "properties": {
          "rating_text": {
            "type": "string",
            "title": "Rating Text"
          },
          "rating_description": {
            "type": "string",
            "title": "Rating Description"
          },
          "x_axis_pos": {
            "type": "number",
            "title": "X Axis Pos"
          },
          "y_axis_pos": {
            "type": "number",
            "title": "Y Axis Pos"
          },
          "analyzed_at": {
            "type": "string",
            "format": "date-time",
            "title": "Analyzed At"
          }
        },
        "type": "object",
        "required": [
          "rating_text",
          "rating_description",
          "x_axis_pos",
          "y_axis_pos",
          "analyzed_at"
        ],
        "title": "PublicAnalysisResponse",
        "description": "Public analysis response model (no sensitive data)."
      },
      "UserRead": {
        "properties": {
          "spotify_id": {
            "type": "string",
            "title": "Spotify Id"
          },
          "email": {
            "type": "string",
            "title": "Email"
          },
          "display_name": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Display Name"
          },
          "country": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Country"
          },
          "image_url": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Image Url"
          },
          "id": {
            "type": "integer",
            "title": "Id"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "title": "Created At"
          },
          "is_active": {
            "type": "boolean",
            "title": "Is Active"
          }
        },
        "type": "object",
        "required": [
          "spotify_id",
          "email",
          "id",
          "created_at",
          "is_active"
        ],
        "title": "UserRead",
        "description": "User read model (public)."
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      }
    },
    "securitySchemes": {
      "HTTPBearer": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  }
}
